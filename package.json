{
  "name": "hgf-helper",
  "description": "A small extension for the Chrome-like browsers that allows you to play mini-games on the .tv/hitsquadgodfather channel",
  "private": true,
  "version": "1.6.2",
  "type": "module",
  "scripts": {
    "dev": "wxt -c ./wxt-dev.config.ts",
    "dev:firefox": "wxt -b firefox -c ./wxt-dev.config.ts",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "tsc --noEmit",
    "postinstall": "wxt prepare",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.309",
    "@types/uikit": "^3.14.5",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "eslint": "^8.35.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-svelte": "^3.5.1",
    "release-it": "^18.1.2",
    "typescript": "^5.8.2",
    "vite-plugin-eslint": "^1.8.1",
    "wxt": "^0.19.29"
  },
  "dependencies": {
    "@lucide/svelte": "^0.503.0",
    "@tailwindcss/vite": "^4.1.4",
    "@wxt-dev/module-svelte": "^2.0.3",
    "clsx": "^2.1.1",
    "firebase": "^11.5.0",
    "reflect-metadata": "^0.2.2",
    "tailwindcss": "^4.1.4",
    "tesseract.js": "^6.0.0",
    "typedi": "^0.10.0",
    "uikit": "^3.23.4"
  }
}
